@model CISSAPortal.Controllers.CurrencyRates
@{
    ViewBag.Title = "Официальный курс валют НБКР";
}

<h2>Официальный курс валют НБКР на @Model.Date</h2>

<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th>
                Валюта
            </th>
            <th>
                Курс в сомах
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var rate in Model.List)
        {
            <tr>
                <td>
                    @rate.ISOCode
                </td>
                <td>
                    @rate.Value
                </td>
            </tr>
        }
    </tbody>
</table>